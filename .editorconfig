# EditorConfig is awesome: https://EditorConfig.org
# top-most EditorConfig file
root = true

[*]
# Unix-style newlines 
end_of_line = lf
# Always end with an empty new line
insert_final_newline = true
# Set default charset to utf-8
charset = utf-8
# Indent with 2 spaces
indent_style = space
indent_size = 2
